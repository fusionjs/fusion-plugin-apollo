{
  "name": "fusion-apollo",
  "description": "FusionJS entry point for React universal rendering /w Apollo",
  "version": "1.6.0",
  "license": "MIT",
  "repository": "fusionjs/fusion-apollo",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "files": [
